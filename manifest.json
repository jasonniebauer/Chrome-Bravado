{
  "name": "Bravado",
  "version": "0.0.0",
  "description": "A chrome extension for replacing your new tab page with a daily view of your tasks.",
  "manifest_version": 2,
  "homepage_url": "https://github.com/jasonniebauer/Chrome-Bravado",
  "icons": {
    "16": "icons/bravado_icon.png"
    // "48": "icons/icon48.png",
    // "128": "icons/icon128.png"
  },
  // "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "geolocation",
    "storage"
  ]
}